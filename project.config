[project]
	description = Injecting maven configurations by composition rather than inheritance
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Richard Vowles (rvowles)
[access "refs/for/refs/*"]
	read = group user/Richard Vowles (rvowles)
	push = group user/Richard Vowles (rvowles)
	label-Code-Review = -2..+2 group user/Richard Vowles (rvowles)
	label-Verified = -1..+1 group user/Richard Vowles (rvowles)
	rebase = group user/Richard Vowles (rvowles)
	removeReviewer = group user/Richard Vowles (rvowles)
	submit = group user/Richard Vowles (rvowles)
[access "refs/heads/*"]
	read = group user/Richard Vowles (rvowles)
	create = group user/Richard Vowles (rvowles)
	push = +force group user/Richard Vowles (rvowles)
	pushMerge = group user/Richard Vowles (rvowles)
[access "refs/tags/*"]
	push = +force group user/Richard Vowles (rvowles)
	pushTag = group user/Richard Vowles (rvowles)
	pushSignedTag = group user/Richard Vowles (rvowles)
